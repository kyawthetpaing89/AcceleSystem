
@{
    ViewBag.Title = "TourokuList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles{
    <link rel="stylesheet" href="@Url.Content("~/ElaAdmin/assets/css/lib/datatable/dataTables.bootstrap.min.css")" />
}

<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col-sm-2 col-md-3">
                <button id="btnAdd" onclick="btnAddClick()" class="btn col-xl-10 text-center" tabindex="38" style="background-color:#1a8cff; color:azure;">
                    経費設定追加
                </button>
            </div>

            <div class="col-sm-7 col-md-9">
                <i class="fa fa-search" style="padding-left:15px;"></i>
                <input type="text" id="myInput" placeholder="経費名入力" class="border border-white" style="width:20%;padding-bottom:10px">
                <table id="tblKeihi" class="tb1 col-sm-12" style="padding-left:0px;">
                    <thead>
                        <tr>
                            <td>経費コード</td>
                            <td>経費名</td>
                            <td>計上単位</td>
                            <td>配賦基準</td>
                        </tr>

                    </thead>

                </table>
            </div>
        </div>
    </div>
</div>

